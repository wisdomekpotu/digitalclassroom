<div class="px-4 py-16 mx-auto sm:max-w-xl md:max-w-full lg:max-w-screen-xl md:px-24 lg:px-8 lg:py-20">
  <div class="grid gap-8 lg:grid-cols-3 sm:max-w-sm sm:mx-auto lg:max-w-full">
    {{#each videos}}

    <div class="flex">
      <div class="pt-1 mr-6 text-center">
        <div class="px-0 border-b border-gray-400">
          <p class="text-lg text-gray-600">{{lecture_day}}</p>
        </div>
        <div class="px-2"><p class="text-lg text-gray-600 font-bold">{{lecture_month}}</p></div>
      </div>
      <div>
        <div class="mb-2">
          <a href="/videos/{{_id}}" class="text-xs font-semibold tracking-wide uppercase transition-colors duration-200 text-gray-600 hover:text-deep-purple-800" aria-label="Category" title="Delevopment">{{lecture_category}}</a>
        </div>
        <div class="mb-2">
          <a href="/" aria-label="Article" class="inline-block text-2xl font-bold leading-5  transition-colors duration-200 hover:text-deep-purple-accent-400">{{title}}</a> 
        </div>
        <p class="text-black">
          Sed ut perspiciatis unde omnis iste natus error sit sed quia consequuntur magni voluptatem doloremque.
        </p>
        <div class="flex items-center">
          <a href="/" aria-label="Author" title="Author" class="mr-3">
            <img src={{image}} alt="avatar" class="object-cover w-10 h-10 rounded-full shadow-sm" />
          </a>
          <div>
            <a href="/" aria-label="Author" title="Author" class="font-semibold text-gray-600 transition-colors duration-200 hover:text-deep-purple-accent-400">wisdom</a>
            <p class="text-sm font-medium leading-4 text-gray-600">{{lecturer_role}}</p>
          </div>
        </div>
      </div>
    </div>

  {{else}}
<p>nope ...wow</p>
{{/each}}

  </div>
</div>

















